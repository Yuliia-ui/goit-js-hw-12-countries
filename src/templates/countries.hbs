
{{#each this }}
 <div class="cards">
     <img src="{{flag}}" alt="Flag of {{capital}}" class="image_card"/>
     <div class="card_list">
         <h1 class="titel">{{name}}</h1>
         <div class="card_text">
             <p class="text_capital">Capital: {{capital}}</p>
             <p class="text_population">Population: {{population}}</p>
             <p class="text_languages">Language: </p>
            <ul class="languages_list">
                {{#each languages}}
                <li class="languages-list__item">{{this.name}}</li>
                {{/each}}
            </ul>
         </div>
     </div>
 </div>
{{/each }}